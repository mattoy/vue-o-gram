{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "fp",
        "@typescript-eslint",
        "vue"
    ],
    "rules": {
    },
    "overrides": [
        {
            "files":"*",
            "rules": {
                "no-mixed-spaces-and-tabs": "off",
                "fp/no-class": "off"
            }
        }
      ]
}
